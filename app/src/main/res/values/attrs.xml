<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultipleSelectionButton">
        <attr name="ascv_unselectedTextColor" format="reference"/>
        <attr name="ascv_unselectedColor" format="reference"/>
        <attr name="ascv_selectedColor" format="reference"/>
        <attr name="ascv_selectedTextColor" format="reference"/>
        <attr name="ascv_items" format="reference"/>
        <attr name="ascv_values" format="reference"/>
        <attr name="ascv_equalWidth" format="boolean"/>
        <attr name="ascv_stretch" format="boolean"/>
        <attr name="ascv_defaultSelection" format="integer"/>
        <attr name="ascv_identifier" format="string"/>
    </declare-styleable>

    <string-array name="search_three_state_option">
        <item>日排行</item>
        <item>週排行</item>
        <item>月排行</item>
    </string-array>

    <string-array name="article_state_option">
        <item>無限制</item>
        <item>最近一週</item>
        <item>最近一月</item>
        <item>最近一年</item>
    </string-array>
</resources>
